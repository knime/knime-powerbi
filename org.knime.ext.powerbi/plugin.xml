<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/powerbi_category.png"
            level-id="powerbi"
            locked="true"
            name="Microsoft Power BI"
            path="/toolintegration">
      </category>
   </extension>
   <extension point="org.knime.workbench.repository.nodes">
      <node
            category-path="/toolintegration/powerbi"
            deprecated="true"
            factory-class="org.knime.ext.powerbi.base.nodes.send.SendToPowerBINodeFactory">
      </node>
      <node
            category-path="/toolintegration/powerbi"
            deprecated="false"
            factory-class="org.knime.ext.powerbi.base.nodes.send2.SendToPowerBINodeFactory2">
      </node>
   </extension>
</plugin>
